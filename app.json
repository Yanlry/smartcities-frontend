{
  "expo": {
    "name": "SmartCities",
    "slug": "smartcities",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#1B5D85",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smartcities.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1B5D85"
      },
      "package": "com.smartcities.app"
    },
    "web": {
      "bundler": "metro"
    },
    "platforms": ["ios", "android", "web"],
    "plugins": [
      "expo-font"
    ]
  }
}